{
  "OSF_R": {
    "handle_hold": false,
    "inputs": [
      "START",
      "R2",
      "L2"
    ],
    "is_normal_mode": true,
    "run_command": "/opt/muos/script/mux/quit.sh reboot osf"
  },
  "OSF_S": {
    "handle_hold": false,
    "inputs": [
      "SELECT",
      "R2",
      "L2"
    ],
    "is_normal_mode": true,
    "run_command": "/opt/muos/script/mux/quit.sh poweroff osf"
  },
  "OSF_M": {
    "is_sequence": true,
    "inputs": [
      "A",
      "B",
      "X",
      "Y",
      "START"
    ],
    "is_normal_mode": true,
    "max_interval": 250,
    "run_command": "mpv /opt/muos/share/media/mustardos.mp3"
  }
}
