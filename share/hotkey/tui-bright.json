{
  "devices": [
    "tui-brick",
    "tui-spoon"
  ],
  "BRIGHT_DOWN": {
    "handle_hold": true,
    "inputs": [
      "SWITCH",
      "VOL_DOWN"
    ],
    "is_handheld_mode": true,
    "is_normal_mode": true,
    "run_command": "/opt/muos/script/device/bright.sh D"
  },
  "BRIGHT_UP": {
    "handle_hold": true,
    "inputs": [
      "SWITCH",
      "VOL_UP"
    ],
    "is_handheld_mode": true,
    "is_normal_mode": true,
    "run_command": "/opt/muos/script/device/bright.sh U"
  }
}
